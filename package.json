{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "lint": "eslint ./app --ext .js,.jsx,.ts,.tsx",
    "web": "expo start --web",
