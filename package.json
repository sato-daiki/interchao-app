    "expo-google-app-auth": "^8.0.1",
    "expo-localization": "^8.0.0",
    "expo-web-browser": "^8.0.0",
    "firebase": "^7.7.0",
    "g": "^2.0.1",
    "i18n-js": "^3.5.1",
    "node-gyp": "^6.1.0",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.0.tar.gz",
